#%RAML 1.0
---
title: local-security
baseUri: http://nidi.guru/raml/{version}
version: v1
securitySchemes:
  - oauth_2_0: !include oauth20.yaml
  - other:
      type: x-other
      describedBy:
        headers:
          AuthorizationReq:
            required: true
          AuthorizationOpt:
            required: false
/sec:
  get:
    securedBy: [null, oauth_2_0]

/uniqueSec:
  get:
    securedBy: [other]
    responses:
      200:

/optSec:
  get:
    securedBy: [null, other]
    responses:
      200:

/doubleSec:
  get:
    securedBy: [oauth_2_0, other]
    responses:
      200:

/unsec:
  get:
    responses:
      200:
